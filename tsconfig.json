{
    "compilerOptions": {
      "module": "CommonJS",
      "target": "ES2021",
      "outDir": "dist",
      "rootDir": "src",
      "esModuleInterop": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "skipLibCheck": true,
      "strictNullChecks": false,
      "noImplicitAny": false,
      "strictBindCallApply": false,
    }
  }
  